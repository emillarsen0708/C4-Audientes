<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/audiogram"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@xml/audiogram_background"
    tools:context=".AudiogramFragment">

    <TextView
        android:id="@+id/sonority_by"
        android:layout_width="187dp"
        android:layout_height="17dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="112dp"
        android:layout_marginLeft="112dp"
        android:layout_marginTop="37dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center_horizontal|center_vertical"
        android:text="SONORITY BY AUDIENTES"
        android:textAppearance="@style/sonority_by2"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/audiogram_header"
        android:layout_width="209dp"
        android:layout_height="38dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="101dp"
        android:layout_marginTop="74dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center_horizontal|center_vertical"
        android:text="AUDIOGRAM"
        android:textAppearance="@style/sonic_distr2"
        android:textStyle="bold"/>

    <Button
        android:id="@+id/component_8"
        android:layout_width="40dp"
        android:layout_height="35dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="352dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/home_house_3"
        android:elevation="5dp"
        />

    <Button
        android:id="@+id/audiogram_rectangle_1"
        android:layout_width="190dp"
        android:layout_height="69dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="200dp"
        android:background="@xml/audiogram_rectangle"
        android:elevation="4dp"
        android:fontFamily="sans-serif-medium"
        android:text="TAKE HEARING TEST"
        android:textAppearance="@style/sonic_distr2"
        android:textSize="16sp"
        android:textStyle="bold"/>

    <Button
        android:id="@+id/audiogram_rectangle_3"
        android:layout_width="190dp"
        android:layout_height="69dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="310dp"
        android:background="@xml/audiogram_rectangle"
        android:elevation="4dp"
        android:fontFamily="sans-serif-medium"
        android:text="UPLOAD AUDIOGRAM"
        android:textAppearance="@style/sonic_distr2"
        android:textSize="16sp"
        android:textStyle="bold"/>

    <Button
        android:id="@+id/audiogram_rectangle_2"
        android:layout_width="190dp"
        android:layout_height="69dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="420dp"
        android:background="@xml/audiogram_rectangle"
        android:elevation="4dp"
        android:fontFamily="sans-serif-medium"
        android:text="VIEW MY AUDIOGRAM"
        android:textAppearance="@style/sonic_distr2"
        android:textSize="16sp"
        android:textStyle="bold"/>



</FrameLayout>
