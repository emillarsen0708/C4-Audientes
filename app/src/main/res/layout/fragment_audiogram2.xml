<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/audiogram"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@xml/frontpage_background_2"
    tools:context=".AudiogramFragment">

    <TextView
        android:id="@+id/sonority_by_audientes"
        android:layout_width="187dp"
        android:layout_height="17dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="112dp"
        android:layout_marginLeft="112dp"
        android:layout_marginTop="37dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center_horizontal|center_vertical"
        android:text="SONORITY BY AUDIENTES"
        android:textAppearance="@style/sonority_by"
        android:textStyle="bold"/>


    <TextView
        android:id="@+id/settings_header"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="90dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center_horizontal"
        android:text="AUDIOGRAM"
        android:textAppearance="@style/home"
        android:textStyle="bold" />

    <Button
        android:id="@+id/audiogram_rectangle_1"
        android:layout_width="190dp"
        android:layout_height="69dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="200dp"
        android:background="@xml/audiogram_rectangle"
        android:elevation="4dp"
        android:fontFamily="sans-serif-medium"
        android:text="TAKE HEARING TEST"
        android:textAppearance="@style/sonic_distr2"
        android:textSize="16sp"
        android:textStyle="bold"/>

    <Button
        android:id="@+id/audiogram_rectangle_3"
        android:layout_width="190dp"
        android:layout_height="69dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="310dp"
        android:background="@xml/audiogram_rectangle"
        android:elevation="4dp"
        android:fontFamily="sans-serif-medium"
        android:text="UPLOAD AUDIOGRAM"
        android:textAppearance="@style/sonic_distr2"
        android:textSize="16sp"
        android:textStyle="bold"/>

    <Button
        android:id="@+id/audiogram_rectangle_2"
        android:layout_width="190dp"
        android:layout_height="69dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="420dp"
        android:background="@xml/audiogram_rectangle"
        android:elevation="4dp"
        android:fontFamily="sans-serif-medium"
        android:text="VIEW MY AUDIOGRAM"
        android:textAppearance="@style/sonic_distr2"
        android:textSize="16sp"
        android:textStyle="bold"/>



</FrameLayout>
