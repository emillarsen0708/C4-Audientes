<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@xml/sound_background"
    tools:context=".PresetFragment">

    <TextView
        android:id="@+id/sonority_by"
        android:layout_width="187dp"
        android:layout_height="17dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="112dp"
        android:layout_marginLeft="112dp"
        android:layout_marginTop="37dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center_horizontal|center_vertical"
        android:text="Sonority By Audientes"
        android:textAppearance="@style/sonority_by2"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/home"
        android:layout_width="130dp"
        android:layout_height="34dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="70dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center_horizontal|center_vertical"
        android:text="Sound"
        android:textAppearance="@style/home"
        android:textColor="#1B325F"
        android:textStyle="bold" />

    <Button
        android:id="@+id/select_preset_button_1"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="130dp"
        android:background="@xml/preset_add_rectangle"
        android:text="Add New"
        android:textAppearance="@style/Play_ambience" />

    <Button
        android:id="@+id/select_preset_button_2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="161dp"
        android:layout_marginTop="130dp"
        android:background="@xml/preset_add_rectangle"
        android:text="Add New"
        android:textAppearance="@style/Play_ambience" />

    <Button
        android:id="@+id/select_preset_button_3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="284dp"
        android:layout_marginTop="130dp"
        android:background="@xml/preset_add_rectangle"
        android:text="Add New"
        android:textAppearance="@style/Play_ambience" />

    <Button
        android:id="@+id/select_preset_button_4"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="284dp"
        android:layout_marginTop="255dp"
        android:background="@xml/preset_add_rectangle"
        android:text="Add New"
        android:textAppearance="@style/Play_ambience" />

    <Button
        android:id="@+id/select_preset_button_5"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="161dp"
        android:layout_marginTop="255dp"
        android:background="@xml/preset_add_rectangle"
        android:text="Add New"
        android:textAppearance="@style/Play_ambience" />

    <Button
        android:id="@+id/select_preset_button_6"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="255dp"
        android:background="@xml/preset_add_rectangle"
        android:text="Add New"
        android:textAppearance="@style/Play_ambience" />

    <Button
        android:id="@+id/select_preset_button_7"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="380dp"
        android:background="@xml/preset_add_rectangle"
        android:text="Add New"
        android:textAppearance="@style/Play_ambience" />

    <Button
        android:id="@+id/select_preset_button_8"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="161dp"
        android:layout_marginTop="380dp"
        android:background="@xml/preset_add_rectangle"
        android:text="Add New"
        android:textAppearance="@style/Play_ambience" />

    <Button
        android:id="@+id/plus_button"
        android:layout_width="66dp"
        android:layout_height="63dp"
        android:layout_marginStart="300dp"
        android:layout_marginTop="400dp"
        android:background="@drawable/ic_plus_icon"
        android:textAppearance="@style/Play_ambience" />

    <Button
        android:id="@+id/edit_preset_button"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="520dp"
        android:background="@drawable/ic_concurrent_marked_icon"
        android:foreground="@drawable/ic_concurrent_icon"
        android:textAppearance="@style/Play_ambience" />

    <TextView
        android:id="@+id/concurrent"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="630dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center_horizontal|center_vertical"
        android:text="Concurrent Mode"
        android:textAppearance="@style/sonority_by2"
        android:textStyle="bold" />

    <Button
        android:id="@+id/play_preset_button"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="520dp"
        android:background="@drawable/ic_continuous_marked_icon"
        android:foreground="@drawable/ic_continuous_icon"
        android:textAppearance="@style/Play_ambience" />
    <TextView
        android:id="@+id/continuous_Mode"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="250dp"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="630dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center_horizontal|center_vertical"
        android:text="Continuous Mode"
        android:textAppearance="@style/sonority_by2"
        android:textStyle="bold" />


</FrameLayout>